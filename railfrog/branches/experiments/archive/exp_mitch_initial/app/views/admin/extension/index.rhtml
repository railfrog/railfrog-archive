<div id='items-list'>
<h2>Extensions</h2>
<p>
<table border=0 width="100%">
<tr>
<th>Name</th>
<th>Type</th>
<th>Options</th>
</tr>
<% @extensions.each do |ext| %>
<tr>
<td><b><%= ext.name %></b><br /><%= ext.desc %></td>
<td align="center"><%= ext['type'] %></td>
<td align="center"></td>
</tr>
<% end %>
</table>
</p>
</div>