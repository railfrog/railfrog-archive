<h2>Users</h2>

<ul>
  <li><%= link_to 'Create new user', fucd_rbac_new_user_url %></li>
</ul>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>&nbsp;</th>
    </tr>
  </thead>
  <tbody>
    <%= render :partial => "user", :collection => @users %>
  </tbody>
</table>