<%= block_start 'labels', 'Labels' %>
  <%= block_nav_start 'labels' %>
    <%= block_nav_action { link_to_remote 'new',
                                          { :url => { :action => 'new_mapping_label',
                                                      :site_mapping_id => site_mapping.id },
                                            :update => 'content-main',
                                            :complete => "Field.focus('mapping_label_name');"
                                          } } %>
  <%= block_nav_end %>

  <%= block_content_start 'labels' %>
    <%= list_labels(site_mapping) %>
  <%= block_content_end %>
<%= block_end %>