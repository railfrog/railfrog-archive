<%= error_messages_for 'user' %>

<dl>
  <dt><label for="user_name">Username</label></dt>
  <dd><%= f.text_field :username %></dd>

  <dt><label for="user_email">Email</label></dt>
  <dd><%= f.text_field :email %></dd>

  <dt><label for="user_first_name">First Name</label></dt>
  <dd><%= f.text_field :first_name %></dd>

  <dt><label for="user_last_name">Last Name</label></dt>
  <dd><%= f.text_field :last_name %></dd>

  <dt><label for="user_password">Password</label></dt>
  <dd><%= f.password_field :password, :value => '' %></dd>

  <dt><label for="user_password_confirmation">Password Confirmation</label></dt>
  <dd><%= f.password_field :password_confirmation, :value => '' %></dd>
</dl>