<h1>Current Users</h1>

<table>
<tr>
<th>Name</th>
<th>Email</th>
</tr>
<% for user in @users %>
<tr>
<td><%= user.first_name %> <%= user.last_name %></td>
<td><%= user.email %></td>
</tr>
<% end %>
</table>
<br />
<%= link_to 'Add a New User', :action => 'new' %><br />
<%= link_to 'Back to Index', :controller => 'index' %>